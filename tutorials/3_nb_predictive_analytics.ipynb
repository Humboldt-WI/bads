      "source": [
        "# root mean squared error:\n",
        "\n",
        "pred_error = np.mean(np.sqrt(res**2))\n",
        "\n",
        "print(\"RMSE of our regression model is {:.4}\".format(pred_error))"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "RMSE of our regression model is 3.102\n"
